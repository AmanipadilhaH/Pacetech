algoritmo "Variaveis"
var
   nome, sobrenome, nomecompleto: caractere

inicio
      Escreval("Informe seu primeiro nome: ")
      leia(nome)
      Escreval("Informe seu sobrenome: ")
      leia(sobrenome)
      nomecompleto <- nome + " " + sobrenome
      Escreval("Seu no me completo é: ", nomecompleto)

fimalgoritmo
